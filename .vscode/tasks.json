{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "buildactionapi",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Kakadu.ActionApi/Kakadu.ActionApi.csproj",
                "/property:GenerateFullPaths=true",
